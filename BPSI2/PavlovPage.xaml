<Page x:Class="BPSI2.PavlovPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BPSI2"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PavlovPage" Background="White">

    <Grid>
        <Button x:Name="mainbutton" Content="Download" HorizontalAlignment="Left" Margin="287,245,0,0" VerticalAlignment="Top" Width="218" Height="58" Click="Button_Click"/>
        <TextBlock x:Name="statusblock" HorizontalAlignment="Left" Margin="110,207,0,0" TextWrapping="Wrap" Text="Status" VerticalAlignment="Top" Height="19" Width="566" TextAlignment="Center"/>
        <ProgressBar x:Name="progressbaryes" HorizontalAlignment="Left" Height="10" Margin="343,226,0,0" VerticalAlignment="Top" Width="100" Foreground="#FF59CAFF" BorderBrush="#FF323232"/>
        <Button x:Name="pushmap" Content="Push A Map" HorizontalAlignment="Left" Margin="356,336,0,0" VerticalAlignment="Top" Width="74" Click="pushmap_Click"/>

    </Grid>
</Page>
